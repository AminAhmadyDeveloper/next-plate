{
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.associations": {
    "trpc": "typescript"
  },

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.vscode": true,
    "**/.next": true,
    "**/api/trpc": true
  },

  "explorer.compactFolders": false,

  "typescript.preferences.importModuleSpecifier": "non-relative"
}
